{
  "name": "@holoscript/multiplayer",
  "version": "1.0.0",
  "description": "VR multiplayer networking - real-time player sync, voice chat, and shared experiences",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./room": {
      "import": {
        "types": "./dist/room.d.ts",
        "default": "./dist/room.js"
      },
      "require": {
        "types": "./dist/room.d.cts",
        "default": "./dist/room.cjs"
      }
    },
    "./player": {
      "import": {
        "types": "./dist/player.d.ts",
        "default": "./dist/player.js"
      },
      "require": {
        "types": "./dist/player.d.cts",
        "default": "./dist/player.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "holoscript",
    "multiplayer",
    "networking",
    "vr",
    "webrtc",
    "websocket",
    "realtime"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  }
}
