{
  "name": "@holoscript/gestures",
  "version": "1.0.0",
  "description": "Hand gesture and body pose recognition for HoloScript VR/AR",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hand": {
      "types": "./dist/hand.d.ts",
      "import": "./dist/hand.js",
      "require": "./dist/hand.cjs"
    },
    "./sequence": {
      "types": "./dist/sequence.d.ts",
      "import": "./dist/sequence.js",
      "require": "./dist/sequence.cjs"
    },
    "./body": {
      "types": "./dist/body.d.ts",
      "import": "./dist/body.js",
      "require": "./dist/body.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "holoscript",
    "gestures",
    "hand-tracking",
    "pose-detection",
    "vr",
    "ar",
    "webxr",
    "body-pose"
  ],
  "author": "HoloScript Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@holoscript/core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@holoscript/core": {
      "optional": true
    }
  }
}
